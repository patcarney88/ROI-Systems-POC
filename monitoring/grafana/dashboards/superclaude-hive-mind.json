{
  "dashboard": {
    "id": null,
    "title": "SuperClaude Hive Mind - Virtual Development Team Monitor",
    "tags": ["superclaude", "hive-mind", "virtual-team", "roi-systems"],
    "style": "dark",
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "Hive Mind Status",
        "type": "stat",
        "targets": [
          {
            "expr": "superclaude_personas_active",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {
                  "color": "red",
                  "value": 0
                },
                {
                  "color": "yellow",
                  "value": 15
                },
                {
                  "color": "green",
                  "value": 22
                }
              ]
            },
            "unit": "short",
            "max": 22,
            "min": 0
          }
        },
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": [
              "lastNotNull"
            ],
            "fields": ""
          },
          "orientation": "auto",
          "textMode": "auto",
          "colorMode": "background",
          "graphMode": "area",
          "justifyMode": "auto"
        },
        "gridPos": {
          "h": 8,
          "w": 6,
          "x": 0,
          "y": 0
        }
      },
      {
        "id": 2,
        "title": "Development Velocity",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(superclaude_tasks_completed_total[5m])",
            "refId": "A",
            "legendFormat": "Tasks/minute"
          }
